{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "useBuiltIns": "usage"
        }
      }
    ]
  ],
  "plugins": [
    [
      "inline-react-svg",
      {
        "svgo": {
          "plugins": [{ "cleanupIDs": false }]
        }
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./src", "./"],
        "alias": {
          "components": "./src/components",
          "page-components": "./src/page-components",
          "store": "./src/store",
          "styling": "./src/styling",
          "utilities": "./src/utilities",
          "layout": "./src/layout",
          "server": "./server",
          "common": "./src/common"
        }
      }
    ],
    ["@babel/plugin-proposal-optional-chaining"]
  ],
  "env": {
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    },
    "dev": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ]
      ]
    },
    "prod": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "ssr": true,
            "displayName": false,
            "fileName": false,
            "preprocess": true
          }
        ]
      ]
    }
  }
}
